{
    "defaultSeverity": "error",
    "indent": [true, "tabs", 2],
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single", "avoid-escape", "avoid-template"],
        "no-console": [false, "log", "error"],
        "curly": [true, "ignore-same-line"],
        "no-var-requires": false,
        "interface-name": [true, "never-prefix"],
        "ordered-imports": false,
        "ban-types":false
    },
    "rulesDirectory": []
}